<title>User Dashboard</title>
  <link rel="stylesheet" href="https://code.getmdl.io/1.1.3/material.orange-indigo.min.css">
  <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
  <script defer src="https://code.getmdl.io/1.1.3/material.min.js"></script>
  <link type="image/x-icon" rel="shortcut icon" href="favicon-1.ico"/>
  <link type="text/css" rel="stylesheet" href="style.css">
        
        
<!-- Container for the demo -->
<div class="mdl-card mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-cell--12-col-tablet mdl-cell--12-col-desktop" id="login-box">
    <div class="mdl-card__title mdl-color--light-green-600 mdl-color-text--white">
        <h2 class="mdl-card__title-text">User Dashboard</h2>
    </div>
    <div class="mdl-card__supporting-text mdl-color-text--grey-600">
        <p style="font-size: large; font-weight: bold;">Welcome, {{name}}!</p>
        <a href="http://flip3.engr.oregonstate.edu:7994/update-account?uid={{{encodedEmail}}}" id="update-account-link">Update Account Details</a>
        <br>
        <img alt="bar chart" src="chart.png" id="user-history-chart">
        <br>
        

        <button class="mdl-button mdl-js-button mdl-button--raised" id="logout-button" onclick="window.location.href = 'http://web.engr.oregonstate.edu/~wrighada/';" >Log Out</button>
    </div>
</div>